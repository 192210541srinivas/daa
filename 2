#include <stdio.h>
#include <math.h>

int is_armstrong(int num) {
    int num_digits = 0;
    int temp = num;
    int sum_cubes = 0;

    while (temp != 0) {
        temp /= 10;
        num_digits++;
    }

    temp = num;
    while (temp != 0) {
        int digit = temp % 10;
        sum_cubes += pow(digit, num_digits);
        temp /= 10;
    }

    return num == sum_cubes;
}

int main() {
    int num;

    printf("Enter a number: ");
    scanf("%d", &num);

    if (is_armstrong(num)) {
        printf("%d is an Armstrong number.\n", num);
    } else {
        printf("%d is not an Armstrong number.\n", num);
    }

    return 0;
}
