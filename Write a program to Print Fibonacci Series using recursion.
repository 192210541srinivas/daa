#include <stdio.h>

int fibonacci(int n) {
    if (n <= 0) {
        printf("Input should be a positive integer.\n");
        return -1; 
    } else if (n == 1) {
        return 0;
    } else if (n == 2) {
        return 1;
    } else {
        return fibonacci(n-1) + fibonacci(n-2);
    }
}

int main() {
    int num_terms;

    printf("Enter the number of terms: ");
    scanf("%d", &num_terms);

    for (int i = 1; i <= num_terms; i++) {
        printf("%d ", fibonacci(i));
    }
    printf("\n");

    return 0;
}
