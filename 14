#include <stdio.h>

int string_length(char str[]) {
    int len = 0;

    while (str[len] != '\0') {
        len++;
    }

    return len;
}

int main() {
    char str[100];
    printf("Enter a string: ");
    scanf("%s", str);
    int len = string_length(str);

    printf("Length of the string: %d\n", len);

    return 0;
}
